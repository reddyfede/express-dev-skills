<%- include("../partials/header") %>
    <h1>
        <%= title %>
    </h1>

    <h3>Fill out the form to add a new skill.</h3>

    <form action="/skills" method="POST" autocomplete="off">
        <div>
            <label for="newskill">Name of the Skill: </label>
            <input id="newskill" type="text" name="skill" required>
        </div>
        <div>
            <label for="rank"> Proficiency X/10: </label>
            <input id="rank" type="number" name="rank" min="1" max="10" required>
        </div>
        <button type="submit">Add Skill</button>
    </form>

    <%- include("../partials/footer") %>